{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      }
    ],
    "env": {
      "PORT": "8080",
      "MONGODB_URI": "@MONGODB_URI",
      "DB_NAME": "@DB_NAME"
    }
  }
  